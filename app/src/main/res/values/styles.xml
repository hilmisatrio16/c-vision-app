<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="ShapeImageAppearanceOverlay">
        <item name="cornerSizeBottomRight">14%</item>
        <item name="cornerSizeBottomLeft">14%</item>
    </style>

    <style name="ShapeItemImageAppearance">
        <item name="cornerSize">12dp</item>
    </style>

    <style name="ShapeImageAppearanceCornerBottom">
        <item name="cornerSizeBottomRight">20dp</item>
        <item name="cornerSizeBottomLeft">20dp</item>
    </style>

    <style name="ShapeImageAppearanceCornerBottomProfile">
        <item name="cornerSizeBottomRight">14dp</item>
        <item name="cornerSizeBottomLeft">14dp</item>
    </style>

    <style name="Widget.App.BottomNavigationView" parent="Widget.MaterialComponents.BottomNavigationView.Colored">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.BottomNavigationView</item>
    </style>

    <style name="ThemeOverlay.App.BottomNavigationView" parent="">
        <item name="colorPrimary">#0D1282</item>
        <item name="colorOnPrimary">#0D1282</item>
    </style>

    <style name="ThemeOverlay.App.BottomSheetDialog" parent="ThemeOverlay.Material3.BottomSheetDialog">
        <item name="android:navigationBarColor" tools:ignore="NewApi">@android:color/transparent
        </item>
    </style>

    <style name="Widget.App.Button.Positive" parent="@style/Widget.Material3.Button.TextButton.Dialog">
        <item name="android:textColor">@color/tuna</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="Widget.App.Button.Negative" parent="@style/Widget.Material3.Button.TextButton.Dialog">
        <item name="android:textColor">@color/red</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

</resources>